package CP_Classes.vo;

import java.util.Calendar;

public class voCoachUserAssignment {
	  int AssignmentPK;
	     int Status;
	     int StartingTime;
		 int EndingTime;
		 int UserFK;
		 int orgID;
		 String Date;
	     String CoachName;
	     String CoachLink;
	     String CoachFileName;
	     String OrganizationName;
	     String SessionVenue;
	     String SessionName;
	     public String getSessionName() {
			return SessionName;
		}
		public void setSessionName(String sessionName) {
			SessionName = sessionName;
		}
		int venueFK;
	     Calendar cal;
	    
		public int getVenueFK() {
			return venueFK;
		}
		public void setVenueFK(int venueFK) {
			this.venueFK = venueFK;
		}
		public String getCoachLink() {
			return CoachLink;
		}
		public void setCoachLink(String coachLink) {
			CoachLink = coachLink;
		}
		public String getCoachFileName() {
			return CoachFileName;
		}
		public void setCoachFileName(String coachFileName) {
			CoachFileName = coachFileName;
		}
		
		public voCoachUserAssignment() {
			super();
		}
		
		public voCoachUserAssignment(int assignmentPK, int status,
				int startingTime, int endingTime, int userFK, int orgID,
				String date, String coachName, String coachLink,
				String coachFileName, String organizationName,
				String sessionVenue, int venueFK, Calendar cal) {
			super();
			AssignmentPK = assignmentPK;
			Status = status;
			StartingTime = startingTime;
			EndingTime = endingTime;
			UserFK = userFK;
			this.orgID = orgID;
			Date = date;
			CoachName = coachName;
			CoachLink = coachLink;
			CoachFileName = coachFileName;
			OrganizationName = organizationName;
			SessionVenue = sessionVenue;
			this.venueFK = venueFK;
			this.cal = cal;
		}
		public int getAssignmentPK() {
			return AssignmentPK;
		}
		public void setAssignmentPK(int assignmentPK) {
			AssignmentPK = assignmentPK;
		}
		public int getStatus() {
			return Status;
		}
		public void setStatus(int status) {
			Status = status;
		}
		public String getDate() {
			return Date;
		}
		public void setDate(String date) {
			Date = date;
		}
		public String getCoachName() {
			return CoachName;
		}
		public void setCoachName(String coachName) {
			CoachName = coachName;
		}
		public String getOrganizationName() {
			return OrganizationName;
		}
		public void setOrganizationName(String organizationName) {
			OrganizationName = organizationName;
		}
		public int getStartingTime() {
			return StartingTime;
		}
		public void setStartingTime(int startingTime) {
			StartingTime = startingTime;
		}
		public int getEndingTime() {
			return EndingTime;
		}
		public void setEndingTime(int endingTime) {
			EndingTime = endingTime;
		}
		public int getUserFK() {
			return UserFK;
		}
		public void setUserFK(int userFK) {
			UserFK = userFK;
		}
		public int getOrgID() {
			return orgID;
		}
		public void setOrgID(int orgID) {
			this.orgID = orgID;
		}
		public String getSessionVenue() {
			return SessionVenue;
		}
		public void setSessionVenue(String sessionVenue) {
			SessionVenue = sessionVenue;
		}
		public Calendar getCal() {
			return cal;
		}
		public void setCal(Calendar cal) {
			this.cal = cal;
		}
		 

}
