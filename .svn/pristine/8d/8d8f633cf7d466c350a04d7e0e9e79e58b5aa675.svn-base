		iFKUser = user;
	}
	public String getIRDEFKUser() {
		return sIRDEFKUser;
	}
	public void setIRDEFKUser(String user) {
		sIRDEFKUser = user;
	}
	public int getPKAssignment() {
		return iPKAssignment;
	}
	public void setPKAssignment(int assignment) {
		iPKAssignment = assignment;
	}
	public int getRRDEFKUser() {
		return iRRDEFKUser;
	}
	public void setRRDEFKUser(int user) {
