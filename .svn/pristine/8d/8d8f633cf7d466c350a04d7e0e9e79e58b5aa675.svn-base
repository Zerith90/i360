package CP_Classes.vo;

public class voAssignment {

	private int	iPKAssignment = 0;
	private int	iFKUser =0;
	private int	iFKJobProfile = 0;
	private String	sIRDEFKUser = "";
	private int	iRRDEFKUser = 0;
	private String	sEntryDate = "";
	private String	sDeadLine = "";
	private int	iStatus	= 0;
	private String sCompletionDate = "";
	
	public String getCompletionDate() {
		return sCompletionDate;
	}
	public void setCompletionDate(String completionDate) {
		sCompletionDate = completionDate;
	}
	public String getDeadLine() {
		return sDeadLine;
	}
	public void setDeadLine(String deadLine) {
		sDeadLine = deadLine;
	}
	public String getEntryDate() {
		return sEntryDate;
	}
	public void setEntryDate(String entryDate) {
		sEntryDate = entryDate;
	}
	public int getFKJobProfile() {
		return iFKJobProfile;
	}
