package CP_Classes.vo;

public class voCluster {
	
	private int iFKCluster = 0;
	private String sClusterName = "";
	
	private int PKCluster = 0;
	private int FKOrganization = 0;
	
	public voCluster(){		
	}
	
	public void setPKCluster(int PKCluster){
		this.PKCluster = PKCluster;
	}
	
	public int getPKCluster(){
		return PKCluster;
	}
	
	public void setClusterName(String name){
		this.sClusterName = name;
	}
	
	public String getClusterName(){
		return sClusterName;
	}
	
	public void setFKOrganization(int FKOrganization){
		this.FKOrganization = FKOrganization;
	}
	
	public int getFKORganization(){
		return FKOrganization;
	}
	
	public void setClusterID(int iFKCluster) {
		this.iFKCluster = iFKCluster;
	}

	public int getClusterID() {
		return iFKCluster;
	}

}
