package CP_Classes.vo;

public class voKeyBehaviour {

	private int iKeyBehaviourID = 0;
	private String[] sKeyBehaviour = new String[6];
	private String sDescription="";
	private int	iPKKeyBehaviour	= 0;
	private int	iFKCompetency	= 0;
	private int	iIsSystemGenerated = 0;
	private int	iKBLevel	= 0;
	private int	iFKCompanyID	= 0;
	private int	iFKOrganizationID = 0;
	
	public int getFKCompanyID() {
		return iFKCompanyID;
	}

	public void setFKCompanyID(int companyID) {
		iFKCompanyID = companyID;
	}

	public int getFKCompetency() {
		return iFKCompetency;
	}

	public void setFKCompetency(int competency) {
		iFKCompetency = competency;
	}

	public int getFKOrganizationID() {
		return iFKOrganizationID;
	}

	public void setFKOrganizationID(int organizationID) {
		iFKOrganizationID = organizationID;
	}

	public int getIsSystemGenerated() {
		return iIsSystemGenerated;
	}

	public void setIsSystemGenerated(int isSystemGenerated) {
		iIsSystemGenerated = isSystemGenerated;
	}

	public int getKBLevel() {
		return iKBLevel;
	}

	public void setKBLevel(int level) {
		iKBLevel = level;
	}

	public int getPKKeyBehaviour() {
		return iPKKeyBehaviour;
	}

	public void setPKKeyBehaviour(int keyBehaviour) {
		iPKKeyBehaviour = keyBehaviour;
	}

	public void setKeyBehaviourID(int ikeyBehaviourID) {
		iKeyBehaviourID = ikeyBehaviourID;
	}
	
	public int getKeyBehaviourID() {
		return iKeyBehaviourID;
	}

	public void setKeyBehaviour(String sKeyBehaviour) {
		this.sKeyBehaviour[0] = sKeyBehaviour;
	}

	public String getKeyBehaviour() {
		return sKeyBehaviour[0];
	}

	public String getDescription() {
		return sDescription;
	}

	public void setDescription(String description) {
		sDescription = description;
	}
	public void setKeyBehaviour(int lang, String sKeyBehaviour){
		this.sKeyBehaviour[lang] = sKeyBehaviour;
	}
	public void setAllKeyBehaviour(String [] sKeyBehaviour){
		this.sKeyBehaviour = sKeyBehaviour;
	}
	
	public String getKeyBehaviour(int lang){
		return sKeyBehaviour[lang];
	}
	public String[] getAllKeyBehaviour(){
		return sKeyBehaviour;
	}
	
	
}
