package CP_Classes.vo;

public class voAllocationMatrix {

	private int	iPKAllocationMatrix	= 0;
	private int	iFKInterviewGuideItem = 0;	
	private int	iFKInterviewers	= 0;
	
	public int getFKInterviewers() {
		return iFKInterviewers;
	}
	public void setFKInterviewers(int interviewers) {
		iFKInterviewers = interviewers;
	}
	public int getFKInterviewGuideItem() {
		return iFKInterviewGuideItem;
	}
	public void setFKInterviewGuideItem(int interviewGuideItem) {
		iFKInterviewGuideItem = interviewGuideItem;
	}
	public int getPKAllocationMatrix() {
		return iPKAllocationMatrix;
	}
	public void setPKAllocationMatrix(int allocationMatrix) {
		iPKAllocationMatrix = allocationMatrix;
	}
}