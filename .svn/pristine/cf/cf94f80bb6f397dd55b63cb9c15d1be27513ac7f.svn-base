package CP_Classes.vo;

public class voJobFunction {
	
	private int	iPKJobFunction	 = 0;
	private String sJobFunctionName	 = "";
	private int	iFKOrganization = 0;
	
	public int getFKOrganization() {
		return iFKOrganization;
	}
	public void setFKOrganization(int organization) {
		iFKOrganization = organization;
	}
	public String getJobFunctionName() {
		return sJobFunctionName;
	}
	public void setJobFunctionName(String jobFunctionName) {
		sJobFunctionName = jobFunctionName;
	}
	public int getPKJobFunction() {
		return iPKJobFunction;
	}
	public void setPKJobFunction(int jobFunction) {
		iPKJobFunction = jobFunction;
	}
	
}
