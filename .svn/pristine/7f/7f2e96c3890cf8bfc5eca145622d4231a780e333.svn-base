package CP_Classes.vo;

public class voRScaleDetail {
	
	private int		iPKRScaleDetail	= 0;
	private int		iFKRScale	= 0;
	private String	sRScaleName	= "";
	private int		iRScaleValue	= 0;
	
	public int getFKRScale() {
		return iFKRScale;
	}
	public void setFKRScale(int scale) {
		iFKRScale = scale;
	}
	public int getPKRScaleDetail() {
		return iPKRScaleDetail;
	}
	public void setPKRScaleDetail(int scaleDetail) {
		iPKRScaleDetail = scaleDetail;
	}
	public String getRScaleName() {
		return sRScaleName;
	}
	public void setRScaleName(String scaleName) {
		sRScaleName = scaleName;
	}
	public int getRScaleValue() {
		return iRScaleValue;
	}
	public void setRScaleValue(int scaleValue) {
		iRScaleValue = scaleValue;
	}
	
	
}
